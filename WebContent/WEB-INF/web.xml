    <description></description>
    <display-name>movieController</display-name>
    <servlet-name>movieController</servlet-name>
    <servlet-class>WebScrap.controller.movieController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>movieController</servlet-name>
    <url-pattern>/movieController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>coronaController</display-name>
    <servlet-name>coronaController</servlet-name>
    <servlet-class>WebScrap.controller.coronaController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>coronaController</servlet-name>
    <url-pattern>/coronaController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>amazonController</display-name>
    <servlet-name>amazonController</servlet-name>
    <servlet-class>WebScrap.controller.amazonController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>amazonController</servlet-name>
    <url-pattern>/amazonController</url-pattern>
  </servlet-mapping>
</web-app>